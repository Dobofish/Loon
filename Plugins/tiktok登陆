[Rewrite]
# 移除廣告
^https:\/\/api-va\.tiktokv\.com\/api\/ad\/ reject
# 解析域名
^https:\/\/tnc\d+-platform-\w+\.tiktokv\.com\/get_domains\/ reject

[Script]
http-request ^https?:\/\/.+\.(tiktokv|tiktokcdn|tiktokapi)\.com\/.+(region\d?=CN|&mcc_mnc=\d+) script-path = https://raw.githubusercontent.com/Semporia/TikTok-Unlock/refs/heads/master/Loon/TikTok.js, tag = TikTok 解鎖

[MITM]
hostname = *.tiktokv.com, *.tiktokcdn.com, *.tiktokapi.com, *.tik-tokapi.com, *.byteoversea.com
